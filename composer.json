{
    "name": "robfrawley/rf-web-app",
    "license": "MIT",
    "type": "project",
    "homepage": "https://src.run/rf-web-app",
    "description": "The core Symfony-based web application powering the robfrawley.com domain.",
    "support": {
        "email": "rmf@src.run",
        "source": "https://src.run/rf-web-app/git",
        "issues": "https://src.run/rf-web-app/issues"
    },
    "keywords": [ "rf", "rmf", "web", "app", "web-app" ],
    "authors": [
        {
            "name": "Rob Frawley 2nd",
            "email": "rmf@src.run",
            "homepage": "https://src.run/rmf",
            "role": "Project Lead"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "erusev/parsedown": "^1.6",
        "erusev/parsedown-extra": "^0.7.1",
        "friendsofsymfony/rest-bundle": "^2.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "javiereguiluz/easyadmin-bundle": "^1.16",
        "jms/serializer-bundle": "^2.0",
        "knplabs/knp-menu-bundle": "^2.1",
        "knplabs/knp-paginator-bundle": "^2.5",
        "liip/imagine-bundle": "^2.0",
        "nabil1337/case-helper": "dev-master",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^3.0.2",
        "src-run/arthur-doctrine-identity-library": "^0.8",
        "src-run/arthur-doctrine-uuid-library": "^0.8",
        "src-run/augustus-exception-library": "^0.8.1",
        "src-run/augustus-utility-library": "^0.7.3",
        "src-run/cocoa-markdown-library": "^0.2.0",
        "src-run/cocoa-word-stem-library": "dev-master",
        "src-run/cocoa-word-stop-library": "dev-master",
        "src-run/vermicious-console-io-library": "^0.2.1",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/assetic-bundle": "^2.8",
        "symfony/cache": "~3.4@dev",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/symfony": "~3.4@dev",
        "symfony/web-server-bundle": "~3.4@dev",
        "twig/twig": "^2.0",
        "whiteoctober/swiftmailerdbbundle": "^1.0"
    },
    "require-dev": {
        "codacy/coverage": "^1.0",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "friendsofphp/php-cs-fixer": "^1.0",
        "fzaninotto/faker": "^1.6",
        "phpunit/phpunit": "^5.5",
        "satooshi/php-coveralls": "^1.0",
        "sensio/generator-bundle": "^3.0",
        "sllh/php-cs-fixer-styleci-bridge": "^2.1",
        "symfony/phpunit-bridge": "~3.4@dev",
        "symfony/var-dumper": "~3.4@dev"
    },
    "autoload": {
        "psr-4": {
            "Rf\\": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rf\\AppBundle\\Tests\\": "tests/AppBundle/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    }
}
