parameters:
    locale: en_US
    database_driver: pdo_mysql
    database_version: 5.6
    database_charset: utf8mb4
    database_collate: utf8mb4_unicode_ci
    env(DB_HOST): 127.0.0.1
    env(DB_PORT): 2525
    env(DB_NAME): rf
    env(DB_USER): root
    env(DB_PASS): null
    mail_transport: smtp
    mail_spool: file
    env(MAIL_HOST): 127.0.0.1
    env(MAIL_PORT): 2525
    env(MAIL_USER): null
    env(MAIL_PASS): null
    env(APP_SECRET): my-public-test-secret
    env(APP_ENV): default
